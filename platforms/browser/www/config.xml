<?xml version='1.0' encoding='utf-8'?>
<widget id="com.lasosearch.mobileapp" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>LasoApp</name>
    <description>
        LASOsearch for mobile
    </description>
    <author email="birm@rbirm.us" href="http://rbirm.us">
        Ryan Birmingham
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
